  <!DOCTYPE html>
<html>
<head>
  <title>Dtechbots</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>

  <script src="//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js"></script>
  <%= csrf_meta_tags %>
</head>
<body class="backgroundColor">



<div class="navbar navbar-fixed-top">  
  <div class="navbar-inner">  
    <div class="container">  
      <ul class="nav nav-pills nav-stacked">  

          <%if current_member%>
              <li><%=link_to "Home", member_pages_index_path,class:"glyphicon glyphicon-home"%></li>
              <li><%=link_to "Inventory",member_pages_inventory_path,class:"glyphicon glyphicon-gift" %>  </li>  
              <li><%=link_to "Manage Orders","",class:"glyphicon" %>  </li>
               <li><%=link_to "Cart",line_items_path,class:"glyphicon glyphicon-shopping-cart" %>  </li>

            <ul class="nav">  
              <li class="dropdown">  
                <a href="#"  
                      class="dropdown-toggle"  
                      data-toggle="dropdown">  
                        <span class="glyphicon glyphicon-user"><%=%></span>     
                      <b class="caret"></b>  
                </a>  
                <ul class="dropdown-menu">  
                    <li><%=link_to " Logout ",member_session_path(current_member),
                                            :method => :delete,class:"glyphicon glyphicon-off" %>  </li>

                </ul>  
              </li>  
           </ul>    
  		     

		</ul>  


    </div>  
  </div>  
</div>
