       <ul class="nav nav-pills nav-stacked">  
        
 
        		  <li><%=link_to "Home",member_pages_index_path,class:"glyphicon glyphicon-home"%></li>      
               <li><%=link_to "Inventory",member_pages_inventory_path,class:"glyphicon glyphicon-gift" %>  </li>   
        			 <li><%=link_to "Orders","",class:"glyphicon glyphicon-shopping-cart" %>  </li>  
