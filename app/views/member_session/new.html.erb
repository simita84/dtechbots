<div class="container-fluid">  
  <div class="row-fluid">  

  	 
     <div class="span5">  
     			<img src="assets/dtech_memberindex.jpg">
     </div>
     <div class="span5"> 
      
		<h1>Log In</h1>


		<%= form_tag member_session_index_path do %>
		  <div class="field">
		    <%= label_tag :username %><br />
		    <%= text_field_tag :username, params[:username] %>
		  </div>
		  <div class="field">
		    <%= label_tag :password %><br />
		    <%= password_field_tag :password %>
		  </div>
		  <div class="actions"><%= submit_tag "Log In" ,:class=>"btn-large"%></div>
		<% end %>
 
      
    </div>
  </div>
    </div>
