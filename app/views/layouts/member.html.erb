               <li><%=link_to " Logout ",member_session_path(current_member),
                                            :method => :delete,class:"glyphicon glyphicon-off" %>  </li>
         
		</ul>  
 
    </div>  
    <div class="span10" >  
    	<%= yield %>
      <!--Body content-->  
      <footer id="footer">
        @Simi Tresa Antony 2014</footer>
    </div>  
  </div>  
</div>  
  


</body>
</html>
